<div id="session-chat-wrapper">
  <div class="row accent-bg p-top p-bot">
    <div class="col-xs-12">
    <div class="media">
      <div class="media-left">
        <img class="forumImage img-circle" src="/assets/img/leaf.png" alt="Admin">
      </div>
      <div class="media-body">
        <h4>Joe Musselman</h4>
      </div>
    </div>
    </div>
