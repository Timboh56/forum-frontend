<div class="navbar fixed navbar-dash navbar-fixed-top navbar-bottom">
  <div class="full-width navbar-container">
    <div class="navbar-left navbar-header">
      <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navCollapse">
        <span class="sr-only">Toggle Navigation</span>
          {{ fa-icon 'leaf' class='font-xlg' }}
      </button>
      <a href="/forum/" class="no-padding-top navbar-brand animated flipInX">
        <img src='assets/img/logos/default.png' class='logo' />
      </a>
      <a href="/" class="navbar-brand animated flipInX">
      </a>
    </div>
    <div class='navbar-middle'>
      <div class='float-left text-right btn-header-container'>
        <div class='hidden-xs visible-sm visible-md visible-lg'>
          {{#link-to 'forum.questions.index' (query-params keywords=''  query=false) class='btn-header'}}
            {{ fa-icon 'list'}} questions
          {{/link-to}}
          {{#link-to 'forum.tags' class='btn-header'}}
            {{ fa-icon 'star'}} tags
          {{/link-to}}
          {{#if currentUser}}
            {{#link-to 'forum.questions.new' class='btn-header'}}
              {{ fa-icon 'edit' }} ask question
            {{/link-to}}
            {{#link-to 'forum.bookmarks' class='btn-header'}}
              {{ fa-icon 'bookmark'}} bookmarks
            {{/link-to}}
            {{#link-to 'user' currentUser class='btn-header'}}
              {{ fa-icon 'user' }} me
            {{/link-to}}
          {{else}}
            {{#link-to 'login' class='btn-header'}}
              {{ fa-icon 'user '}} login
            {{/link-to}}
          {{/if}}
        </div>
      </div>
    </div>
    <div class='navbar-right hidden-xs hidden-sm visible-md visible-lg visible-xlg'>
      <div class='float-right'>
        {{#link-to 'login' class='btn-header gear border-none float-right'}}
          Dashboard {{ fa-icon 'gear' class='font-xlg' }}
        {{/link-to}}
      </div>
    </div>
  </div>
  <div class='top-nav-second-row'>
    <div class='search-forums-container'>
      {{ search-forums searchItems=searchItems model=model }}
    </div>
  </div>
</div>
