{{ forum-header forumHeaderLabel='Most active'goToLink='goToLink' tabs=tabs}}

<div class="tab-content p-top">
  <div class='container-fluid'>
    <div role="tabpanel" class="row tab-panel active" id="qa">
      <div class="col-sm-12">
        <div id="mainTable">
          <table class="table-simple">
            <tbody>
              {{#each sortedByActivityCount as |question|}}
                {{ question-container question=question }}
              {{/each}}
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
</div>
